            document.getElementById("header").className = "header_area";
            return false;
        }

        function eventFire(el, etype) {
            if (el.fireEvent) {
                el.fireEvent('on' + etype);
            } else {
                var evObj = document.createEvent('Events');
                evObj.initEvent(etype, true, false);
                el.dispatchEvent(evObj);
            }
        }

        function LoginBtnClc() {
            namestr = document.getElementById('<%= UserName.ClientID %>').value.length;
                    passstr = document.getElementById('<%= UserPassword.ClientID %>').value.length;
            let k = 0
            if (namestr == '') {
                document.getElementById("invMsgUserNM").className = "invBox alert alert-danger";
                document.getElementById("invMsgUserNM").innerHTML = "name should contain at least 1 symbol";

            } else {
                k++;
                document.getElementById("invMsgUserNM").className = "invisible";
                document.getElementById("invMsgUserNM").innerHTML = " ";
            }

            if (passstr == '') {
                document.getElementById("invMsgUserPSW").className = "invBox alert alert-danger";
                document.getElementById("invMsgUserPSW").innerHTML = "password should contain at least 1 symbol";
                return false;
            } else {
                k++;
                document.getElementById("invMsgUserPSW").className = "invisible";
                document.getElementById("invMsgUserPSW").innerHTML = " ";
            }

            return k == 2;
        }

        function RestoreBtnClc() {
            namestr = document.getElementById('<%= UserName.ClientID %>').value.length;
            document.getElementById("invMsgUserPSW").className = "invisible";
            document.getElementById("invMsgUserPSW").innerHTML = " ";

